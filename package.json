{
  "name": "dream-opp-backend",
  "version": "1.0.0",
  "description": "Serving ReactJS and React Native clients everywhere. Made with Koa.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "resetdb": "dropdb dream-opp; createdb dream-opp; psql -d dream-opp -f schema.sql; psql -d dream-opp -f data.sql;"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-koa": "^1.3.6",
    "dotenv": "^6.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.4",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "nodemon": "^1.17.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  }
}
